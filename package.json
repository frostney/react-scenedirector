{
  "name": "react-scenedirector",
  "version": "1.1.3",
  "description": "Scene management in React",
  "main": "dist/react-scenedirector.js",
  "jsnext:main": "dist/react-scenedirector.es2015.js",
  "scripts": {
    "build": "rollup-babel-lib-bundler src/index.js",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "lint": "eslint src",
    "precommit": "npm run lint",
    "test": "ava --verbose 'src/**/*.test.js'",
    "coverage": "node --require babel-register ./node_modules/.bin/nyc --all --reporter lcov --reporter html npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostney/react-scenedirector.git"
  },
  "keywords": [
    "react",
    "scene",
    "screen",
    "scenedirector"
  ],
  "files": [
    "dist",
    "src"
  ],
  "ava": {
    "babel": "inherit",
    "require": "./ava-jsdom-setup"
  },
  "nyc": {
    "exclude": [
      "coverage",
      "dist",
      "**/*.test.js",
      "ava-jsdom-setup.js"
    ]
  },
  "rollupBabelLibBundler": {
    "moduleName": "SceneDirector",
    "babel": {
      "presets": [
        "es2015-loose-rollup",
        "react"
      ],
      "plugins": [
        "transform-class-properties"
      ],
      "externalHelpers": false
    }
  },
  "author": "Johannes Stein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frostney/react-scenedirector/issues"
  },
  "homepage": "https://github.com/frostney/react-scenedirector#readme",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-core": "^6.7.2",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-loose-rollup": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.1.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "husky": "^0.11.4",
    "in-publish": "^2.0.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.3",
    "nyc": "^6.1.1",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "rollup-babel-lib-bundler": "^2.5.5",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "react": "^0.14.7"
  }
}
